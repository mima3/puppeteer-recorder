{
  "name": "puppeteer-recorder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "vue-cli-service lint",
    "build": "vue-cli-service electron:build",
    "serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "record": "node src/cli/record",
    "replay": "node src/cli/replay",
    "generate": "node src/cli/generate"
  },
  "main": "background.js",
  "dependencies": {
    "argparse": "^2.0.1",
    "core-js": "^3.8.3",
    "electron-log": "^4.4.8",
    "html-validate": "^7.12.0",
    "puppeteer": "^19.4.1",
    "puppeteer-chromium-resolver": "^19.1.0",
    "vue": "^3.2.13",
    "vue-cli-plugin-electron-builder": "^3.0.0-alpha.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "^5.0.8",
    "electron": "^22.0.0",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
